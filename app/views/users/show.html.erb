<div id="user">
  <div class="container-fluid">
    <div class="row">
      <div class="col-sm-9">
        <div class="row">
          <div class="col-md-3">
            <div class="thumbnail">
              <%= image_tag("thumbnail.svg") %>
            </div>
            <h1><%= @user.name %></h1>
            <h4><%= @user.username %></h4>
          </div>
          <div class="col-md-8 col-md-offset-1">
            


          </div>
        </div>
      </div>
      <div class="col-sm-3">
        <%= render partial: 'layouts/sidebar', locals: { creations: @creations, users: @users } %>
      </div>
    </div>
  </div>
</div>